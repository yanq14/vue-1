<!DOCTYPE html>
<html>
    <head>
    <meta http-equiv="Content-Type" content="text/html;charset=UTF-8">
    <title>导航栏实例</title>
    <script src="https://cdn.jsdelivr.net/npm/vue@2.5.16/dist/vue.js"></script>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootswatch/4.1.2/minty/bootstrap.min.css">
    </head>
    <body>
        <div id="content">
            <!--导航栏-->
            <ul id="navigation" class="nav nav-tabs">
                <li class="nav-item">
                    <a data-toggle="tab" href="#dairy-post-info" class="nav-link">{{navigation[0].content}}</a>
                </li>
                <li class="nav-item">
                    <a data-toggle="tab" href="#dairy-post-info" class="nav-link">{{navigation[1].content}}</a>
                </li>
                <li class="nav-item">
                    <a data-toggle="tab" href="#dairy-post-info" class="nav-link">{{navigation[2].content}}</a>
                </li>
            </ul>
        </div>
        <script>
            var contentApp=new Vue({
                el:"#content",
                data:{
                    navigation:[
                        {
                            content:"日记",
                        },
                        {
                            content:"游记",
                        },
                        {
                            content:"照片集",
                        }
                    ],
                }
            })
        </script>
    </body>
